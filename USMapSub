
public class BigCityObject implements Comparable<BigCityObject> {
	
	private String bigCityName;
	private int bigCityPop;

	public BigCityObject(String bigCityName, int parseInt) {
		this.bigCityName = bigCityName;
		this.bigCityPop = parseInt;
	}

	public String getBigCityName() {
		return bigCityName;
	}

	public int getBigCityPop() {
		return bigCityPop;
	}
	
	public int compareTo(BigCityObject o1) {
        // Ascending
        if (getBigCityPop() > o1.getBigCityPop()) {
            return 1;
        } else if (getBigCityPop() == o1.getBigCityPop()) {
            return 0;
        }
        return -1;
    }   

}
